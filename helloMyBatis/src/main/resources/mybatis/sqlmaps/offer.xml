<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.book">
    <select id="getOfferList" resultType="com.spring.book.model.Offer">
        SELECT
            id,
            name,
            email,
            text
      	FROM
            offers;
    </select>
</mapper>